
@using Projet2Homechef.Models
@model List<Projet2Homechef.Models.Prestation.Commande>

@{
    ViewBag.Title = "PrestationChef";
	
}
<h2><img id ="imgTitre" src="/images/sousmenu presta.png"  /></h2>
        <div class="divider-custom">
			<h2>Mes services en cours</h2>
			
		</div>
<table class="styled-table">

      <tr>
          <th>Id</th>    
          <th>Titre</th>  
          <th>Description</th>
          <th>Client</th> 
          <th>   </th>
          
         
       </tr>
    @foreach(var commande in Model)
    {
         <tr>
          <td>@commande.Id</td>    
          <td>@commande.Service.Titre</td>
          <td>@commande.Service.Description</td>
          <td>@commande.Villageois.UserName</td>
          
          <td> <button1 class="btn btn-success btn" type="submit">@Html.ActionLink("Détail", "DetailPrestationChef", "Prestation",new{@id = @commande.Id}) </button1> </td>

      </tr>
      
  
      
    }
</table>


